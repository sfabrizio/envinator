{
  "name": "envinator",
  "version": "0.4.0",
  "description": "NPM module enviroment generator",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "nyc jasmine-node --verbose --captureExceptions test",
    "test:watch": "nyc jasmine-node --verbose --autotest --captureExceptions test",
    "coverage": "nyc report --reporter=text-lcov > coverage.json"
  },
  "devDependencies": {
    "jasmine-node": "^1.14.5",
    "nyc": "^11.0.2"
  },
  "engines": {
    "node": "7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sfabrizio/envinator.git"
  },
  "bin": {
    "envinator": "bin/cli.js"
  },
  "dependencies": {
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "figlet": "^1.2.0",
    "inquirer": "^3.0.6",
    "shelljs": "^0.7.5"
  },
  "keywords": [
    "envinator",
    "generator",
    "npm package"
  ],
  "author": "Samuel Fabrizio  <samuelfabrizio@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sfabrizio/envinator/issues"
  },
  "homepage": "https://github.com/sfabrizio/envinator#readme"
}
